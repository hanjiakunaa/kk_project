{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://v1.hitokoto.cn/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy2"
    }
  ]
}
