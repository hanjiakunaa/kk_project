{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://v1.hitokoto.cn/$1"
    },
    {
      "source": "/kuleu/(.*)",
      "destination": "https://api.kuleu.com/api/$1"
    },
    {
      "source": "/pexelsPicAndVideo/(.*)",
      "destination": "https://api.pexels.com/$1"
    }
  ]
}
